{
	"name": "bingo",
	"private": true,
	"scripts": {
		"build:web": "npm run build --workspace=web",
		"build:ws": "npm run build --workspace=ws",
		"build": "npm run build:web && npm run build:ws",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"start:web": "npm run start --workspace=web",
		"start:ws": "npm run start --workspace=ws",
		"start": "npm run start:web & npm run start:ws & wait"
	},
	"devDependencies": {
		"@types/uuid": "^10.0.0",
		"prettier": "^3.2.5",
		"turbo": "^2.5.6",
		"typescript": "^5.4.5"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@12.0.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"dependencies": {
		"dotenv": "^17.2.1",
		"framer-motion": "^11.11.13",
		"use-sound": "^4.0.3",
		"uuid": "^10.0.0",
		"vaul": "^1.0.0"
	}
}